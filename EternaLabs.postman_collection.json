          "raw": "{\n  \"tokenIn\": \"SOL\",\n  \"tokenOut\": \"USDC\",\n  \"amount\": 1.5\n}"
        },
        "url": {
          "raw": "https://eterna-labs-backend-69zf.onrender.com/api/orders/execute",
          "protocol": "https",
          "host": [
            "eterna-labs-backend-69zf",
            "onrender",
            "com"
          ],
          "path": [
            "api",
            "orders",
            "execute"
          ]
        }
      },
      "response": []
    },
    {
      "name": "WebSocket - Order Updates",
      "event": [
        {
          "listen": "prerequest",
          "script": {
            "exec": [
              "// Set your orderId manually before connecting.",
              "pm.environment.set(\"orderId\", \"enter_order_id_here\");"
